{
  "name": "pinesu",
  "version": "1.0.0",
  "description": "NodeJS application which provides file version control along with integrity checking on the Ethereum blockchain network",
  "main": "index.js",
  "type": "module",
  "keywords": [
    "cli",
    "Git"
  ],
  "bin": {
    "pinesu": "./index.js"
  },
  "preferGlobal": true,
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "startServer": "npm --prefix dapp/ run serve",
    "dev": "concurrently --handle-input --kill-others \"npm run startServer\" \"npm run start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plspeziali/PineSU.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/plspeziali/PineSU/issues"
  },
  "homepage": "https://github.com/plspeziali/PineSU#readme",
  "dependencies": {
    "adm-zip": "^0.5.5",
    "async": "^3.2.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "figlet": "^1.5.0",
    "hashmap": "^2.4.0",
    "hidefile": "^3.0.0",
    "inquirer": "^8.0.0",
    "macaddress": "^0.5.1",
    "merkle-calendar": "^1.0.0",
    "merkle-tools": "^1.4.1",
    "mkdirp": "^1.0.4",
    "nexmo": "^2.9.1",
    "opener": "^1.5.2",
    "ora": "^5.4.0",
    "path": "^0.12.7",
    "simple-git": "^3.5.0",
    "touch": "^3.1.0",
    "underscore": "^1.12.1",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.12",
    "web3": "^1.3.3"
  },
  "devDependencies": {
    "concurrently": "^6.0.1"
  }
}
